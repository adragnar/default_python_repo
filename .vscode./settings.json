{
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.copilotmd": "vscode.markdown.preview.editor",
        "{[0-9][0-9][0-9][0-9]}-{[0-9][0-9]}-{[0-9][0-9]}T{[0-9][0-9]}[:-]{[0-9][0-9]}[:-]{[0-9][0-9]}*{[A-Za-z0-9]{21}}*.json": "inspect-ai.log-editor"
    },

    "files.exclude": {
        "**/node_modules/**": true,
        "**/.mypy_cache/**": true,
        "**/__pycache__/**": true,
        "**/.pytest_cache/**": true,
        "**/__init__.py": true,
        "README.md": true,
        ".gitignore": true,
        ".env": true,
        "setup.sh": true,
        ".python-version": true
    }
}
